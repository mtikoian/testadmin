  <Code>Public Function ConvertMultivalueParameterToCommaDelimitedList(ByVal Parameter as Parameter) as String
	Dim s as String 
	If parameter.IsMultiValue then
		s = "" 
		For i as integer = 0 to parameter.Count-1
			s = s + CStr(parameter.Value(i))
			If i &lt;&gt; parameter.Count-1 Then s = s + ","
		Next
	Else
		s = "Single value: " + CStr(parameter.Value)
	End If
	Return s
End Function</Code>
