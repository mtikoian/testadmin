      <Query>
        <DataSourceName>SQLdetails</DataSourceName>
        <CommandText>;WITH SQLdetails_CTE(event_name, timestamp, source_database_id, object_id, object_type, cpu, duration, waiting, reads, writes, session_id, sql_text, plan_handle, tsql_stack) AS 
(
	SELECT	target_data.value('(event/@name)[1]', 'varchar(30)'),
			target_data.value('(event/@timestamp)[1]', 'datetime2'),
			target_data.value('(event/data/value)[1]', 'int'),
			target_data.value('(event/data/value)[2]', 'int'),
			target_data.value('(event/data/value)[3]', 'int'),
			target_data.value('(event/data/value)[4]', 'int'), 
			target_data.value('(event/data/value)[5]', 'int'), 
			(target_data.value('(event/data/value)[5]', 'int')/1000) - (target_data.value('(event/data/value)[4]', 'int')),
			target_data.value('(event/data/value)[6]', 'int'), 
			target_data.value('(event/data/value)[7]', 'int'), 
			target_data.value('(event/action/value)[1]', 'int'),
			target_data.value('(event/action/value)[2]', 'varchar(max)'),
			target_data.value('(event/action/value)[3]', 'varchar(max)'),
			target_data.value('(event/action/value)[4]', 'varchar(max)')
			FROM dbo.XEtable 
			WHERE target_data.value('(event/@name)[1]', 'varchar(30)') = 'sql_statement_completed'
	UNION 
			SELECT	target_data.value('(event/@name)[1]', 'varchar(30)'),
			target_data.value('(event/@timestamp)[1]', 'datetime2'),
			target_data.value('(event/data/value)[1]', 'int'),
			target_data.value('(event/data/value)[2]', 'int'),
			target_data.value('(event/data/value)[3]', 'int'),
			target_data.value('(event/data/value)[4]', 'int'), 
			target_data.value('(event/data/value)[5]', 'int'), 
			(target_data.value('(event/data/value)[5]', 'int')/1000 - target_data.value('(event/data/value)[4]', 'int')) ,
			target_data.value('(event/data/value)[6]', 'int'), 
			target_data.value('(event/data/value)[7]', 'int'), 
			target_data.value('(event/action/value)[1]', 'int'),
			target_data.value('(event/action/value)[2]', 'varchar(max)'),
			target_data.value('(event/action/value)[3]', 'varchar(max)'),
			target_data.value('(event/action/value)[4]', 'varchar(max)')
			FROM dbo.XEtable 
			WHERE target_data.value('(event/@name)[1]', 'varchar(30)') = 'sp_statement_completed'
)
SELECT * FROM SQLdetails_CTE
WHERE 	timestamp = @p_EndTime
AND         tsql_stack = @p_tsql_stack</CommandText>
        <QueryParameters>
          <QueryParameter Name="@p_EndTime">
            <Value>=Parameters!p_EndTime.Value</Value>
