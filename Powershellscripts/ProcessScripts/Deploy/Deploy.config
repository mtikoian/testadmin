<configuration productName="SDNH">
	<package>
		<directories>
			<directory name="Website" pathRelativeToScript="..\Dss.Sdnh.Web.WebUi" />
			<directory name="WcfService" pathRelativeToScript="..\Dss.Sdnh.App.ServiceWcfHost" />
			<directory name="LoggingService" pathRelativeToScript="..\Dss.Framework.ServiceHost" />
			<directory name="Database" pathRelativeToScript="..\Database" />
			<directory name="BatchInvoker" pathRelativeToScript="..\Dss.Sdnh.App.BatchInvoker" />
		</directories>
		<assembly name="DomainObject" pathRelativeToScript="..\Dss.Sdnh.App.ServiceWcfHost\bin\Dss.Sdnh.App.Assembler.dll"/>
	</package>
	<reconfigTemplates>
		<!-- If reconfigAttribute is not present, text of the xpath will be replaced -->
		<template name="website">
			<keys>
				<xpath name="SdnhService" value="/configuration/appSettings/add[@key='ISdnhService']" reconfigAttribute="value" />
				<xpath name="LoggingService" value="/configuration/appSettings/add[@key='ILoggingService']" reconfigAttribute="value" />
				<xpath name="SsnPublicKey" value="/configuration/appSettings/add[@key='Ssn']" reconfigAttribute="value" />
				<xpath name="Last4SsnPublicKey" value="/configuration/appSettings/add[@key='Last4Ssn']" reconfigAttribute="value" />
				<xpath name="SsnSaltBytes" value="/configuration/appSettings/add[@key='SsnSaltBytes']" reconfigAttribute="value" />
			</keys>
		</template>
    <template name="batchInvoker">
			<keys>
				<xpath name="BatchJobErrorEmailSubject" value="/configuration/appSettings/add[@key='BatchJobErrorEmailSubject']" reconfigAttribute="value" />
				<xpath name="DssSenderEmailAddressDisplayText" value="/configuration/appSettings/add[@key='DssSenderEmailAddressDisplayText']" reconfigAttribute="value" />
				<xpath name="DssSmtpServerHost" value="/configuration/appSettings/add[@key='DssSmtpServerHost']" reconfigAttribute="value" />
				<xpath name="CryptographyService" value="/configuration/appSettings/add[@key='ICryptographyService']" reconfigAttribute="value" />
				<xpath name="ApplicationId" value="/configuration/appSettings/add[@key='ApplicationId']" reconfigAttribute="value" />
				<xpath name="ApplicationToken" value="/configuration/appSettings/add[@key='ApplicationToken']" reconfigAttribute="value" />
				<xpath name="DebugMode" value="/configuration/appSettings/add[@key='DebugMode']" reconfigAttribute="value" />
				<xpath name="MigrateEmployers" value="/configuration/appSettings/add[@key='MigrateEmployers']" reconfigAttribute="value" />
				<xpath name="CoreFtpExePath" value="/configuration/appSettings/add[@key='CoreFtpExePath']" reconfigAttribute="value" />
				<xpath name="SftpSiteName" value="/configuration/appSettings/add[@key='SftpSiteName']" reconfigAttribute="value" />
				<xpath name="BatchLogDirectory" value="/configuration/appSettings/add[@key='BatchLogDirectory']" reconfigAttribute="value" />
        <xpath name="SsnPublicKey" value="/configuration/appSettings/add[@key='Ssn']" reconfigAttribute="value" />
        <xpath name="Last4SsnPublicKey" value="/configuration/appSettings/add[@key='Last4Ssn']" reconfigAttribute="value" />
        <xpath name="SsnSaltBytes" value="/configuration/appSettings/add[@key='SsnSaltBytes']" reconfigAttribute="value" />
        <xpath name="ConnectionString" value="/configuration/connectionStrings/add[@name='SdnhDbContext']" reconfigAttribute="connectionString"/>
        <xpath name="LoggingServiceConnection" value="/configuration/connectionStrings/add[@name='DbConnectionString']" reconfigAttribute="connectionString"/>
			</keys>
		</template>
		<template name="wcfService">
			<keys>
				<xpath name="AllowUsers" value="/configuration/system.web/authorization/allow" reconfigAttribute="users"/>				
				<xpath name="Endpoint" value="/configuration/system.serviceModel/services/service/endpoint" reconfigAttribute="address"/>				
				<xpath name="IdentityDns" value="/configuration/system.serviceModel/services/service/endpoint/identity/dns" reconfigAttribute="value"/>
				<xpath name="ConnectionString" value="/configuration/connectionStrings/add[@name='SdnhDbContext']" reconfigAttribute="connectionString"/>		
				<xpath name="LoggingServiceConnection" value="/configuration/connectionStrings/add[@name='DbConnectionString']" reconfigAttribute="connectionString"/>
				<xpath name="SenderEmail" value="/configuration/appSettings/add[@key='DssSenderEmailAddress']" reconfigAttribute="value"/>
				<xpath name="TechTeamEmail" value="/configuration/appSettings/add[@key='TechnicalTeamEmails']" reconfigAttribute="value"/>
				<xpath name="SenderEmailAddressDisplayText" value="/configuration/appSettings/add[@key='DssSenderEmailAddressDisplayText']" reconfigAttribute="value"/>
				<xpath name="DssSmtpServerHost" value="/configuration/appSettings/add[@key='DssSmtpServerHost']" reconfigAttribute="value"/>
